<table style="width:100%;text-align: center;background-color:#fff;border-radius:10px;" align="center" cellpadding="0" cellspacing="0">
  <tr>
    <td><img src="<%= edmTpl.bannerUrl %>" alt=""></td>
  </tr>
  <tr height="90">
  </tr>
  <% edmTpl.themes.forEach(function(theme){  %>

    <tr>
      <td style="font-size:50px;font-family:'微软雅黑','sans-serif';color:#26252d;"><%= theme %> </td>
    </tr>

  <% }) %>
  <tr height="80"></tr>

  <% edmTpl.contents.forEach(function(content){  %>

    <tr height="50">
      <td style="font-size:28px;font-family:'微软雅黑','sans-serif';color:#7e8188;">
        <%= content %>
      </td>
    </tr>

  <% }) %>
  <tr height="90"></tr>
  <tr>
    <td>
      <a href="<%= edmTpl.btnUrl.linkUrl %>"><img src="<%= edmTpl.btnUrl.imgUrl %>" alt=""></a>
    </td>
  </tr>
  <tr height="90"></tr>
</table>