 <table style="width:100%;text-align: center;background-color:#fff;border-radius:10px;margin-top:70px;" align="center" cellpadding="0" cellspacing="0">
    <tr height="90"></tr>
    <tr>
      <td style="font-size:50px;font-family:'微软雅黑','sans-serif';color:#26252d;">
        <%= footer.intro.title %>
      </td>
    </tr>
    <tr height="80"></tr>


    <% footer.intro.contents.forEach(function(content,index){ %>

      <% var size = footer.intro.contents.length %>

      <% content.contentTexts.forEach(function(contentText,index){ %>
        <tr height="50">
          <td style="font-size:35px;font-family:'微软雅黑','sans-serif';color:#d95273;">
            <%= contentText.text %> 
          </td>
        </tr>
      <%  }) %>

      <% if(index < size - 1){ %>
        <tr height="40"></tr>
        <tr>
          <td><img src="<%= footer.intro.lineUrl %> " alt=""></td>
        </tr>
        <tr height="40"></tr>
      <%  } %>
    <%  }) %>


    <tr height="90"></tr>
  </table>